{
    "env": {
        "browser": true,
        "es2020": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": 0
    }
}
